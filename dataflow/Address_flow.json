{
	"name": "Address_flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SalesLTCustomerAddress",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     addresstype as string,",
				"     addresstype as string ($addresstype)",
				"}",
				"source(output(",
				"          {CustomerID     AddressID     AddressType     rowguid     ModifiedDate} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(addresstype ==$addresstype) ~> filter1"
			]
		}
	}
}